FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      LAD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    xMin
    {
        type            fixedValue;
        value           uniform 0;
    }
    xMax
    {
        type            fixedValue;
        value           uniform 0;
    }
    yMin
    {
        type            fixedValue;
        value           uniform 0;
    }
    yMax
    {
        type            fixedValue;
        value           uniform 0;
    }
    zMax
    {
        type            fixedValue;
        value           uniform 0;
    }
    ground
    {
        type            fixedValue;
        value           uniform 0;
    }
}


